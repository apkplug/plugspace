    start-level ="1"
    start-up="true"
    Bundle-Activator="com.apkplug.cam360plug.MainActivitor"
    plug_id="plug_user_20uxDq9pzUI"
    Use-Host-MetaData="true"
>
    <processor
        uri="apkplug://cam360/start"
        className="com.apkplug.cam360plug.StartProcessor"
    />

    <rpc-processor
        uri="apkplug://cam360/rpc/start"
        className="com.apkplug.cam360plug.RPCCam360Start"
        />

</plugin-features>